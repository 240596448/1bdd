		.ЗависитОт("asserts")
		.ЗависитОт("1commands")
		.ЗависитОт("fs")
		.ВключитьФайл("src")
		.ВключитьФайл("tests")
		.ВключитьФайл("features")
		.ВключитьФайл(".vscode")
		.ВключитьФайл("readme.md")
